{
  "name": "tabso-api",
  "version": "1.0.0",
  "main": "server.js",
  "author": "kno@kno.com.es",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel --config-file ./babel.config.json -d build --copy-files src/",
    "babel-version": "babel --version",
    "build": "yarn clean && yarn babel-version && yarn build-server",
    "start": "node build/server.js",
    "dev": "nodemon --exec babel-node src/server.js",
    "db": "docker run -d --rm --name mariadb -v ${PWD}/data:/var/lib/mysql -e MYSQL_ROOT_PASSWORD=1234 -p 3306:3306 mariadb"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*",
      "lib/*"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5",
    "urlsafe-base64": "^1.0.0",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-plugin-add-import-extension": "^1.4.3",
    "eslint": "^7.12.1",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": "v14.15.0",
    "yarn": "1.22.5"
  }
}
